# EditorConfig - http://editorconfig.org/
# The usage of this plugin is encouraged to keep coding style consistent
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false
indent_style = tab

[*.json]
indent_style = space
indent_size = 2

[*.yml]
indent_style = space
indent_size = 2
